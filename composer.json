{
  "name": "5andhalf/ipaas-lib-laravel",
  "type": "library",
  "license": "proprietary",
  "description": "IPaaS package for laravel provides php support for google app-engine logging and handling",
  "homepage": "https://github.com/calcinai/xero-php",
  "minimum-stability": "dev",
  "prefer-stable": true,

  "require": {
    "php": "^7.1.3",
    "google/cloud": "^0.59",
    "guzzlehttp/guzzle": "^6.3",
    "laravel/framework": "^5.6"
  },

  "require-dev": {
    "phpunit/phpunit": "~4.7"
  },

  "autoload": {
    "psr-4": {
      "Ipaas\\": "."
    }
  },

  "extra": {
    "laravel": {
      "providers": [
        "Ipaas\\IpaasServiceProvider"
      ]
    }
  }
}

{
  "name": "ipaas/gapp-laravel",
  "type": "library",
  "license": "CC-BY-NC-ND-4.0",
  "description": "IPaaS package for laravel provides php support for google app-engine logging and handling",
  "homepage": "https://5andhalf.com/",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1.3",
    "laravel/framework": "^5.6",
    "guzzlehttp/guzzle": "^6.3",
    "google/cloud": "0.60.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "squizlabs/php_codesniffer": "*",
    "orchestra/testbench": "^3.8"
  },
  "autoload": {
    "files": [
      "Helper/ipaas-helpers.php",
      "Helper/exception-helpers.php",
      "Helper/Converter.php"
    ],
    "psr-4": {
      "Ipaas\\Gapp\\": "."
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ipaas\\Gapp\\Tests\\": "tests"
    }
  }
}
